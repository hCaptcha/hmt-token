{
  "name": "hmt-token",
  "version": "1.0.0",
  "description": "Human Token (HMT) powering HUMAN Protocol",
  "main": "truffle.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "compile": "truffle compile; truffle migrate",
    "fix:js": "eslint --fix test/** migrations/**",
    "lint:js": "eslint test/** migrations/**",
    "lint:sol": "solhint contracts/*.sol contracts/*/*.sol test/*.sol test/*/*.sol",
    "lint": "yarn lint:js && yarn lint:sol",
    "publish": "truffle publish",
    "pretest": "yarn lint",
    "test": "truffle test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hCaptcha/hmt-token.git"
  },
  "keywords": [
    "ethereum"
  ],
  "authors": [
    "HUMAN Protocol"
  ],
  "license": "MIT",
  "homepage": "https://github.com/hCaptcha/hmt-token#readme",
  "dependencies": {
    "truffle-hdwallet-provider": "0.0.5"
  },
  "devDependencies": {
    "dotenv": "^6.0.0",
    "eslint": "^6.0.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-mocha": "^5.3.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-react": "^7.14.1",
    "solhint": "^1.2.1"
  }
}
